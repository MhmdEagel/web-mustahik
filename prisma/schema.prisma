generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Mustahik {
  id String @id @default(uuid())
  nik String
  nama String
  nomorTelepon String
  alamat String
  jenisBantuan String
  jumlahBantuan Int
  tanggal DateTime
  namaPenerimaLaporan String
  status String
}
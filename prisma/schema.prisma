generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(cuid())
  email String @unique
  password String
}

model Mustahik {
  id String @id @default(cuid())
  NIK String
  nama String
  nomor_telepon String
  alamat String
  jenis_bantuan String
  jumlah_bantuan Int
  tanggal DateTime
  nama_penerima_laporan String
  status String
}